    "url": "https://github.com/MiroDojkic/vue-reform/issues"
  },
  "homepage": "https://github.com/MiroDojkic/vue-reform#readme",
  "devDependencies": {
    "bili": "^5.0.5",
    "eslint-config-xo-vue": "^3.1.22",
    "eslint-plugin-vue": "^6.2.2",
    "np": "^6.5.0",
    "poi": "^12.10.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "xo": "^0.33.1"
  },
  "dependencies": {
    "vee-validate": "^3.3.9"
